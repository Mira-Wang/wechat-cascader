.tab_item {
  margin-right: 30rpx;
  font-size: 32rpx;
  position: relative;
  height: 6vh;
  line-height: 6vh;
}

.tab_item_active {
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 3px;
  background-color: #2691FF;
  border-radius: 3px;
  transition-duration: 0.3s;
}


.tree_picker_swiper {
  height: 52vh;
  overflow-y: scroll;
}

.tree_picker_swiper_item {
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
}

.tree_picker_swiper_item::after {
  position: absolute;
  content: '';
  bottom: -2rpx;
  left: 0;
  width: 100%;
  height: 2rpx;
  border-top: 1px solid #e8e8e8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.swiper_item_li_active {
  color: #2691FF
}