.area_box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  padding: 36rpx 32rpx;

}

.area_name {
  font-size: 34rpx;
  /* font-weight: bold; */
  color: #212121;
}

.change_box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  color: #2691FF;
}

.change_icon {
  width: 37rpx;
  height: 31rpx;
  margin-left: 14rpx;
}

.bottom_btn_box {
  width: 100%;
  padding: 20rpx 0;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
}

.bottom_btn {
  width: 688rpx;
  height: 80rpx;
  background: #2691FF;
  color: #fff;
  border-radius: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  align-content: center;
}

.list_box {
  padding-bottom: 100rpx;
}

.item {
  padding: 40rpx 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  position: relative;
}

.item::after {
  position: absolute;
  content: '';
  bottom: -2rpx;
  left: 0;
  width: 100%;
  height: 2rpx;
  border-top: 1px solid #e8e8e8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  /*缩放*/
}

.name_box {
  font-size: 32rpx;
  font-weight: 500;

}

.name {
  color: #333333;
  margin-bottom: 20rpx;
}

.status {
  width: 60rpx;
  padding: 6rpx 0;
  background: #E2F0FF;
  border-radius: 8rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #2691FF;
  text-align: center;
}

.disabled .name {
  color: #999999;
}

.disabled .status {
  background: #C8C8C8;
  color: #fff;
}


.btn {
  background: #2691FF;
  width: 170rpx;
  height: 66rpx;
  border-radius: 66rpx;
  font-size: 28rpx;
  color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  align-content: center;
}

.disabled_btn {
  background: #C8C8C8;
  width: 170rpx;
  height: 66rpx;
  border-radius: 66rpx;
  color: #fff;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  align-content: center;
}

.select_mask_block {
  box-sizing: border-box;
  -webkit-font-smoothing: antialiased;
  width: 100%;
  height: 70vh;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-radius: 16rpx 16rpx 0px 0px;
  box-sizing: border-box;
  padding: 3vh 32rpx;
}

.head_block {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  font-size: 32rpx;
  height: 5vh;
}

.community {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
}

.community_name {
  color: #2691FF;
  margin-left: 20rpx;
}

.area {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
}

.mask_area_name {
  color: #2691FF;
  margin-left: 20rpx;
}

.tree_picker_box {
  height: 59vh;

}

.tree_picker_head {
  height: 6vh;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  align-content: center;
  position: relative;
}

